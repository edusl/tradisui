extends includes/_tpl-base.pug

block title
  title Confirmación alta

block body

  - var headerMenu = false
  - var footer = 'footer'


  body
    

    .main-global

      include includes/_tpl-header.pug


      main.main-content


        .main-content__wrap

          .confirm-head

            h1.title-confirm Estamos tramitando los datos de su cuenta

            .confirm-head__text
              p Hemo recibido la información correctamete. En breve le comunicaremos el estado de su cuenta.

            a.link.link--primary.link--icon.space-between.grid-a-2(href='#')
              span Continuar
              svg.icon.rotate-180
                use(xlink:href='../static/img/common/svg/svg-symbols.svg#arrow-circle')


      include includes/_tpl-footer.pug
